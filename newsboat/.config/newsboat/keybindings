# unbind keys
unbind-key ENTER
unbind-key j
unbind-key k
unbind-key J
unbind-key K

# bind keys - vim style
bind-key / open-search tagselection
bind-key < prev articlelist
bind-key ESC quit
bind-key G end
bind-key J next-feed articlelist
bind-key N prev-unread
bind-key R reload-all
bind-key U show-urls
bind-key a toggle-article-read
bind-key d pagedown
bind-key g home
bind-key h quit
bind-key j down
bind-key k up
bind-key l open
bind-key n next-unread
bind-key u pageup
bind-key > next articlelist
bind-key K prev-feed articlelist
